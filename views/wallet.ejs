  <script>
    $(document).ready(function () {
      $('#addMoneyBtn').click(function () {
        var amount = $('#moneyAmount').val();
        if (!amount) {
          $("#errorMsg").text("Please enter the amount you want to add.");
          return;
        }

        $.ajax({
          url: '/addWalletMoney',
          type: 'POST',
          data: { amount: amount },
        })
          .then(res => {
            if (res.err) {
              return location.href = res.url;
            }

            if (res.success == true) {
              const options = {
                key: res.key_id,
                amount: res.wallet.amount,
                currency: "INR",
                name: "I Wear",
                description: "Test Transaction",
                order_id: res.wallet.id,
                callback_url: "/addedMoney",
                prefill: {
                  name: "Shubham Sunil Patil",
                  email: "shubhampatil5333@gmail.com",
                  contact: "7558826132"
                },
                notes: {
                  address: "Razorpay Corporate Office"
                },
                theme: {
                  color: "#3399cc"
                }
              };

              const rzp1 = new Razorpay(options);

              rzp1.open();
            }
          })
          .catch(err => {
            console.log(err)
          })
      });
    });
  </script>








